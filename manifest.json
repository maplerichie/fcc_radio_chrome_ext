{
  "name": "freeCodeCamp.org Code Radio",
  "description": "Code Radio. 24/7 music designed for coding",
  "version": "1.2.0",
  "manifest_version": 2,
  "permissions": ["notifications", "background", "storage"],
  "background": {
    "scripts": ["background.js"],
    "persistent": true
  },
  "browser_action": {
    "default_title": "Code Radio",
    "default_icon": "images/16.png",
    "default_popup": "popup.html"
  },
  "icons": {
    "16": "images/16.png",
    "32": "images/32.png",
    "48": "images/48.png",
    "128": "images/128.png"
  }
}
